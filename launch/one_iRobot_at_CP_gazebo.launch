<launch>
    <include file="$(find irobotcreate2)/launch/empty_world.launch">
        <arg name="paused" default="false"/>
        <arg name="use_sim_time" default="true"/>
        <arg name="gui" default="true"/>
        <arg name="headless" default="false"/>
        <arg name="debug" default="false"/>
        <arg name="verbose" default="false"/>
        <arg name="world_name" default="iRobot_worlds/one_iRobot_at_CP.world"/>
    </include>
    
   
    <node name="laser_static_tf" pkg="tf" type="static_transform_publisher" args="0 0 0.05 0 0 0 /iRobot_0/base_link /iRobot_0/hokuyo_laser_link 500" />   
    
</launch>

